  <div class="swiper-wrapper">
    <!-- Slides -->
      {% for i in 1..10 %}
        <div class="swiper-slide">
          <img src="/src/img/portfolio-photo/{{ i }}.jpg" alt="" loading="lazy">
          <div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
        </div>
      {% endfor %}
  </div>
